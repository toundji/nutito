    "nestjs-config": "^1.4.10",
    "nestjs-redoc": "^2.2.2",
    "nodemailer": "^6.7.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.6.1",
    "pg": "^8.7.3",
    "qrcode": "^1.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.5",
    "socket.io-redis": "^6.1.1",
    "sql.js": "^1.6.2",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "twig": "^1.15.4",
    "typeorm": "^0.2.38",
    "typescript": "^4.3.5"
